<style>
    html, body {
        padding: 0;
        margin: 0;
    }

    #preloader {
        position: absolute;
        width: 100%;
        height: 100%;
        background-color: #ff9800;
    }

    .preloader-circle {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);

        width: 100px;
        height: 100px;
    }

    .preloader-circle .preloader-child {
        width: 100%;
        height: 100%;
        position: absolute;
        left: 0;
        top: 0;
    }

    .preloader-circle .preloader-child:before {
        content: '';
        display: block;
        margin: 0 auto;
        width: 15%;
        height: 15%;
        background-color: #fff;
        border-radius: 100%;
        -webkit-animation: preloader-circleBounceDelay 1.2s infinite ease-in-out both;
        animation: preloader-circleBounceDelay 1.2s infinite ease-in-out both;
    }

    .preloader-circle .preloader-circle2 {
        -webkit-transform: rotate(30deg);
        -ms-transform: rotate(30deg);
        transform: rotate(30deg);
    }

    .preloader-circle .preloader-circle3 {
        -webkit-transform: rotate(60deg);
        -ms-transform: rotate(60deg);
        transform: rotate(60deg);
    }

    .preloader-circle .preloader-circle4 {
        -webkit-transform: rotate(90deg);
        -ms-transform: rotate(90deg);
        transform: rotate(90deg);
    }

    .preloader-circle .preloader-circle5 {
        -webkit-transform: rotate(120deg);
        -ms-transform: rotate(120deg);
        transform: rotate(120deg);
